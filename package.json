{
  "name": "prospect-activity-ms",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "build": "yarn tsc && npm run lint",
    "compile": "yarn tsc",
    "local": "cross-env ENV=LOCAL node dist/index.js",
    "start:dev": "cross-env ENV=DEV node dist/index.js",
    "start:uat": "ENV=UAT node dist/index.js",
    "test": "cross-env ENV=LOCAL jest src --config=jest.config.js --coverage  --colors --reporters=default --reporters=jest-junit --coverageReporters=lcov && node ./process-coverage-report.js",
    "server": "cross-env ENV=production node dist/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "start": "cross-env NODE_ENV=production && yarn build && nodemon --tls-min-v1.0 ./src/server.ts ",
    "build:production": "webpack --mode production --config webpack.prod.config.js && rimraf dist && rimraf webpack-modules && yarn tsc -p .",
    "lint": "tsc --noEmit && eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test:api:local": "yarn create:reportfolder && cross-env ENV=LOCAL cross-env BRAND=ABC cross-env APP_PORT=8085 cross-env API_VERSION=1 qa-suite --tags @functional-component --features test-automation/api/functional-tests/features --stepDefinitions test-automation/api/functional-tests/stepdefinitions --configDir test-automation/config/test-module --atids '' --output test-automation/api/functional-tests/reports/cucumberOutput.json && yarn test:genarate:report",
    "test:api:dev": "yarn create:reportfolder && cross-env ENV=DEV cross-env BRAND=ABC cross-env APP_PORT=8085 cross-env API_VERSION=1 qa-suite --tags @functional-component --features test-automation/api/functional-tests/features --stepDefinitions test-automation/api/functional-tests/stepdefinitions --configDir test-automation/config/test-module   --atids '' --output test-automation/api/functional-tests/reports/cucumberOutput.json && yarn test:genarate:report",
    "test:api:uat": "yarn create:reportfolder && cross-env ENV=UAT cross-env BRAND=ABC cross-env APP_PORT=8085 cross-env API_VERSION=1 qa-suite --tags @integration-tests --features test-automation/api/functional-tests/features --stepDefinitions test-automation/api/functional-tests/stepdefinitions --configDir test-automation/config/test-module   --atids '' --output test-automation/api/functional-tests/reports/cucumberOutput.json && yarn test:genarate:report",
    "test:contract:consumer": "cross-env ENV=DEV cross-env BRAND=ABC jest --config=jest.automantion.config.js test-automation/api/contract-tests/consumer-contracts/ --testTimeout=20000 --runInBand --force-exit",
    "test:contract:provider": "cross-env ENV=DEV cross-env BRAND=ABC jest --config=jest.automantion.config.js test-automation/api/contract-tests/provider-verify/ --testTimeout=200000 --runInBand --force-exit",
    "create:reportfolder": "mkdirp test-automation/api/functional-tests/reports",
    "test:genarate:report": "qa-generate-report --input ./test-automation/api/functional-tests/reports/cucumberOutput.json --output ./test-automation/api/functional-tests/reports/activity-ms-report.html"
  },
  "dependencies": {
    "@types/config": "^0.0.41",
    "@types/joi": "^17.2.3",
    "@types/mssql": "^8.1.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "async": "3.2.4",
    "axios": "^1.3.2",
    "axios-logger": "^2.6.1",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "helmet": "^6.1.5",
    "https": "^1.0.0",
    "jest-junit": "^16.0.0",
    "joi": "^17.6.0",
    "jsonpath": "^1.1.1",
    "mssql": "^9.0.1",
    "path": "^0.12.7",
    "pino-pretty": "^9.2.0",
    "pino-std-serializers": "^6.1.0",
    "serialize-error": "^11.0.0",
    "swagger-ui-express": "^4.4.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "vsts-coverage-styles": "^1.0.9",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@faker-js/faker": "^7.6.0",
    "@lbg/mortgages-automation": "6.0.0",
    "@pact-foundation/pact": "9.17.3",
    "@types/async": "3.2.5",
    "@types/axios": "^0.14.0",
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "4.2.14",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.12",
    "@types/typescript": "^2.0.0",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aes256": "^1.1.0",
    "babel-loader": "^8.2.2",
    "chai": "4.3.0",
    "chai-as-promised": "7.1.1",
    "copy-modules-webpack-plugin": "^2.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.4",
    "supertest": "6.1.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^8.10.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint && yarn run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "resolutions": {
    "glob-parent": "^6.0.1",
    "decode-uri-component": "0.2.1",
    "got": "12.1.0",
    "loader-utils": "2.0.4",
    "node-notifier": "9.0.0",
    "qs": "6.11.0",
    "semver-regex": "3.1.4",
    "**/@babel/parser": "^7.16.0",
    "**/@babel/helper-builder-binary-assignment-operator-visitor": "^7.16.0",
    "**/@babel/helper-remap-async-to-generator": "^7.16.0",
    "**/moment": "^2.29.2"
  },
  "overrides": {
    "jsonwebtoken": "^9.0.0"
  }
}
